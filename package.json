{
  "private": true,
  "name": "bare-expo",
  "description": "Example of embedding Bare in an Expo application",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "pod": "npx pod-install",
    "ios": "expo run:ios",
    "gen": " npx bare-pack --out app/main.bundle.js main.js --target ios-arm64 --target ios-arm64-simulator --target ios-x64-simulator --target android-arm --target android-arm64 --target android-ia32 --target android-x64 --linked"
  },
